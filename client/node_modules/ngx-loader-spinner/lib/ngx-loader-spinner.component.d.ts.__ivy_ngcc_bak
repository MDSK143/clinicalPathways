import { OnInit } from '@angular/core';
import { NgxLoaderSpinnerService } from './ngx-loader-spinner.service';
export declare class NgxLoaderSpinnerComponent implements OnInit {
    private loader;
    /**
     * Loader flag
     */
    isLoading: boolean;
    constructor(loader: NgxLoaderSpinnerService);
    ngOnInit(): void;
    /**
     * Get loader status
     */
    private getLoaderStatus;
}
